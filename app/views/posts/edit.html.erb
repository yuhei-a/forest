

<%= form_with model:@post, url:post_path(@post), local:true do |f| %>

 <%= f.label "タイトル" %>
 <%= f.text_field :title %><br>

 <%= f.label "画象" %>
 <%= f.attachment_field :post_image %><br>

 <%= f.label "タグ" %>
 <%= f.text_field :name, value: @tag_list %><br>

 <%= f.label "本文" %>
 <%= f.text_field :content %><br>

 <%= f.submit "更新" %><br>

<% end %>