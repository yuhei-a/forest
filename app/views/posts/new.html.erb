
  <%= form_with model:@postnew, url:posts_path, local:true do |f| %>

   <%= f.label "タイトル" %>
   <%= f.text_field :title %><br>

   <%= f.label "画象" %>
   <%= f.attachment_field :post_image %><br>

   <%= f.label "タグ" %>
   <%= f.text_field :name %><br>

   <%= f.label "本文" %>
   <%= f.text_field :content %><br>

   <%= f.submit "投稿する" %><br>

  <% end %>
