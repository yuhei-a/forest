<%= form_with model:@user, url:user_path(@user), local:true do |f| %>

 <%= f.label "name" %>
 <%= f.text_field :name %>

 <%= f.label "introduction" %>
 <%= f.text_field :introduction %>

 <%= f.label "gender" %>
 <%= f.select :gender, User.genders.keys, include_blank: true %>

 <%= f.label "prefectures" %>
 <%= f.select :prefectures, User.prefectures.keys, include_blank: true %>

 <%= f.label "bloodtype" %>
 <%= f.select :bloodtype, User.bloodtypes.keys, include_blank: true %>

 <%= f.label "sign" %>
 <%= f.select :sign, User.signs.keys, include_blank: true %>


 <%= f.submit "編集" %>

<% end %>

